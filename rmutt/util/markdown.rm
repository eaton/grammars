// Markdown formatting helpers

H1[text]: "# " text " #\n";
H2[text]: "## " text " ##\n";
H3[text]: "### " text " ###\n";
LIST[text]: text;
LI[text]: "* " text "\n";
QUOTE[text]: "\n> " text "\n";
IMG[url, title]: "![" title "](" url ")";
LINK[url, text]: "[" text "](" url ")";
EM[text]: "*" text "*";
STRONG[text]: "**" text "**";
STRIKE[text]: "~" text "~";
P[text]: "\n" text "\n";
BR: "\n";